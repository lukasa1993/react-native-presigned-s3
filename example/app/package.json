{
  "name": "PS3App",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 14 Pro'",
    "lint": "eslint .",
    "start": "react-native start --resetCache",
    "postinstall": "cross-os postinstall",
    "test": "jest"
  },
  "cross-os": {
    "postinstall": {
      "darwin": "jetifier -r && cd ios && USE_FABRIC=0 RCT_NEW_ARCH_ENABLED=0 pod update",
      "linux": "jetifier -r",
      "win32": "jetifier -r"
    }
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.17.12",
    "@react-navigation/native": "6.1.6",
    "@react-navigation/native-stack": "6.9.12",
    "@tanstack/react-query": "4.26.1",
    "nanoid": "4.0.1",
    "path-browserify": "1.0.1",
    "qs": "6.11.1",
    "react": "18.2.0",
    "react-native": "0.71.4",
    "react-native-background-upload": "6.6.0",
    "react-native-file-viewer": "2.1.5",
    "react-native-fs": "2.20.0",
    "react-native-image-crop-picker": "0.39.0",
    "react-native-presigned-s3": "file:../../src",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "3.20.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.3",
    "@babel/preset-env": "7.20.2",
    "@babel/runtime": "7.21.0",
    "@react-native-community/eslint-config": "3.2.0",
    "@tsconfig/react-native": "2.0.3",
    "@types/jest": "29.4.4",
    "@types/qs": "6.9.7",
    "@types/react": "18.0.28",
    "@types/react-test-renderer": "18.0.0",
    "@types/node": "18.15.3",
    "cross-os": "1.5.0",
    "eslint": "8.36.0",
    "jetifier": "2.0.0",
    "metro-react-native-babel-preset": "0.76.0",
    "prettier": "2.8.4",
    "react-test-renderer": "18.2.0",
    "typescript": "4.9.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
